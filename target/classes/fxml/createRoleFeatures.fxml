<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="305.0" prefWidth="325.0" stylesheets="@../styles/admin.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="invsys.controllers.createrolefeatures.CreateRoleFeaturesController">
   <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
   </padding>
   <children>
      <JFXTextField fx:id="funcNameField" layoutX="31.0" layoutY="125.0" prefHeight="28.0" prefWidth="264.0" promptText="Create Func Name" />
      <JFXButton layoutX="14.0" layoutY="245.0" onAction="#createFunc" prefHeight="28.0" prefWidth="85.0" styleClass="menu-buttons-selected" text="Save" />
      <JFXButton layoutX="217.0" layoutY="245.0" onAction="#clear" prefHeight="28.0" prefWidth="78.0" styleClass="menu-buttons-selected" text="Clear" />
      <JFXComboBox fx:id="parentFunCombo" layoutX="30.0" layoutY="73.0" prefHeight="28.0" prefWidth="264.0" promptText="Parent Functions" />
      <JFXButton layoutX="111.0" layoutY="245.0" onAction="#updateFunc" prefHeight="28.0" prefWidth="85.0" styleClass="menu-buttons-selected" text="Update" />
      <JFXButton layoutX="214.0" layoutY="192.0" onAction="#deletFunc" prefHeight="28.0" prefWidth="85.0" styleClass="menu-buttons-selected" text="Delete" />
      <JFXComboBox fx:id="funcTypeCombo" layoutX="31.0" layoutY="24.0" prefHeight="28.0" prefWidth="264.0" promptText="Function Type" />
   </children>
</AnchorPane>
